{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.js"
    },
    {
      "source": "/op",
      "destination": "/api/op.js"
    },
    {
      "source": "/script",
      "destination": "/api/script.lua.js"
    },
    {
      "source": "/(.*)",
      "destination": "/site/$1"
    }
  ]
}
